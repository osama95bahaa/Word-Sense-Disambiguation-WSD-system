   "source": [
    "#print(ss.fileids())\n",
    "#print(ss.instances('hard.pos'))\n",
    "\n",
    "## Forming 3 lists each containing the context words of each sense (HARD1, HARD2 and HARD3) excluding stopwords and lowercase\n",
    "\n",
    "def sense_instances(instances, sense):\n",
    "    return [instance.context for instance in instances if instance.senses[0]==sense]\n",
    "\n",
    "\n",
    "stop = set(stopwords.words('english'))\n",
    "\n",
    "sense1 = sense_instances(ss.instances('hard.pos'), 'HARD1')\n",
    "senses1Context =[]\n",
    "for i in sense1:\n",
    "    for x in i:\n",
    "        if(x[0] not in stop):\n",
    "            senses1Context.append(x[0].lower())\n",
    "\n",
    "sense2 = sense_instances(ss.instances('hard.pos'), 'HARD2')\n",
    "senses2Context =[]\n",
    "for i in sense2:\n",
    "    for x in i:\n",
    "        if(x[0] not in stop):\n",
    "            senses2Context.append(x[0].lower())\n",
    "\n",
    "sense3 = sense_instances(ss.instances('hard.pos'), 'HARD3')\n",
    "senses3Context =[]\n",
    "for i in sense3:\n",
    "    for x in i:\n",
    "        if(x[0] not in stop):\n",
    "            senses3Context.append(x[0].lower())\n",
    "\n",
    "        \n",
    "print(senses1Context, '\\n')\n",
    "print(senses2Context, '\\n')\n",
    "print(senses3Context)"
   ]
  },
