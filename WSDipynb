   "source": [
    "sentence1 = \"This job is a very hard and difficult task\"\n",
    "sent1 = list(sentence1.split())\n",
    "print('Test Sentence1 :',sentence1)\n",
    "\n",
    "sent1Intersectsense1 = len(set(sent1).intersection(senses1Context))\n",
    "sent1Intersectsense2 = len(set(sent1).intersection(senses2Context))\n",
    "sent1Intersectsense3 = len(set(sent1).intersection(senses3Context))\n",
    "\n",
    "if((sent1Intersectsense1 >= sent1Intersectsense2) and (sent1Intersectsense1 >= sent1Intersectsense3)):\n",
    "    print('The correct sense for this sentence is HARD1')\n",
    "elif((sent1Intersectsense2 >= sent1Intersectsense1) and (sent1Intersectsense2 >= sent1Intersectsense3)):\n",
    "    print('The correct sense for this sentence is HARD2')\n",
    "else:\n",
    "    print('The correct sense for this sentence is HARD3')"
   ]
